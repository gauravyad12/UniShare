                "margin-top": "100px",
                "@media (max-width: 992px)": {
                  "margin-top": "48px"
                },
                ".mbr-text": {
                  "display": "inline-block",
                  "width": "50%",
                  "margin-bottom": "0",
                  "@media (max-width: 992px)": {
                    "width": "100%"
                  },
                  "span": {
                    "background": "linear-gradient(90deg, @grad, lighten(@grad, 10%) 65%, lighten(@grad, 20%))",
                    "-webkit-background-clip": "text",
                    "-webkit-text-fill-color": "transparent"
                  }
                }
              },
              ".mbr-section-btn": {
                "margin-top": "36px"
              }
            },
            ".slider-wrapper": {
