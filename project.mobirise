                      "display": "block"
                    },
                    ".text-wrap": {
                      "display": "flex",
                      "flex-direction": "column",
                      "justify-content": "center",
                      "margin-right": "16px",
                      "width": "100%",
                      "@media (max-width: 992px)": {
                        "width": "100%",
                        "margin-right": "0",
                        "margin-bottom": "16px"
                      },
                      ".item-title": {
                        "margin-bottom": "4px"
                      },
                      ".item-text": {
                        "margin-bottom": "0"
                      }
                    },
                    ".icon-wrapper": {
                      "min-width": "24px",
                      "display": "flex",
                      "align-items": "flex-end",
                      "height": "100%",
                      "@media (max-width: 992px)": {
                        "min-width": "auto"
